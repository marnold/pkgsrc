#! /bin/sh
#
# $NetBSD: canna,v 1.1.1.1 2002/05/31 13:00:58 seb Exp $
#
# PROVIDE: canna
# REQUIRE: DAEMON

name="canna"
command_start="@PREFIX@/sbin/cannaserver"
pidfile=""

cmd=${1:-start}

case ${cmd} in
start)
    if [ -x ${command_start} ]; then
        echo "Starting ${name}."
        ${command_start} >/dev/null 2>&1
    fi
    ;;
*)
    :
    ;;
esac
exit 0
