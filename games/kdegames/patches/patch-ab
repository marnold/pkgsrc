*** 36,45 ****
  ########### File Formats ###########
  
! .SUFFIXES: .cpp
  
  .cpp.o:
  	$(CXX) -c $(CXXFLAGS) $<
  
! %.moc: %.h
  	$(MOC) $< -o $@
  
--- 36,45 ----
  ########### File Formats ###########
  
! .SUFFIXES: .cpp .moc
  
  .cpp.o:
  	$(CXX) -c $(CXXFLAGS) $<
  
! .h.moc:
  	$(MOC) $< -o $@
  
