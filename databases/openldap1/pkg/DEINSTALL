#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1.1.1 2001/10/25 18:38:13 kleink Exp $
#

case $2 in
        DEINSTALL) cat << EOF
===========================================================================
If you don't want to use openldap anymore, remove ${PKG_PREFIX}/etc/openldap.
===========================================================================

EOF
        ;;
esac
