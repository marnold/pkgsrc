+++ soltools/mkdepend/def.h	2009-09-12 20:56:00.000000000 +0900
@@ -151,7 +151,7 @@
 
 char			*copy();
 char			*base_name();
-char			*getline();
+char			*ooo_getline();
 struct symtab		*slookup();
 struct symtab		*isdefined();
