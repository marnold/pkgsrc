 # STLIBOBJS	list of .o objects; this must not contain variable
 #		references.
 
-.SUFFIXES: .c .so .po
+.SUFFIXES: .c .so .po .lo
 .c.so:
 	$(CC) $(PICFLAGS) $(ALL_CFLAGS) -c $< -o $*.so.o && $(MV) $*.so.o $*.so
 .c.po:
 	$(CC) $(PROFFLAGS) $(ALL_CFLAGS) -c $< -o $*.po.o && $(MV) $*.po.o $*.po
+.c.lo:
+	$(LIBTOOL) --mode=compile $(CC) $(ALL_CFLAGS) -c $<
 
