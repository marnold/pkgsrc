#!/bin/sh
#
# $NetBSD: uname,v 1.1.1.1 2000/08/25 01:07:29 jlam Exp $
#
# This emulates just enough of Linux uname to fool the installer.

case $1 in
-n)	/usr/bin/uname -n
	;;
-v)	echo 6.x		# SuSE 6.x
	;;
-r)	echo 2.0.37		# Linux kernel 2.0.37
	;;
-m)	/usr/bin/uname -m
	;;
*)	/usr/bin/uname $*
	;;
esac
