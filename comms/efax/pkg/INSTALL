#!/bin/sh
#
test -d /var/spool ||  \
	( mkdir /var/spool ; chmod 755 /var/spool )
test -d /var/spool/efax ||  \
	( mkdir /var/spool/efax ; chmod 755 /var/spool/efax )
test -d /var/log ||  \
	( mkdir /var/log ; chmod 755 /var/log )
test -d /var/log/efax ||  \
	( mkdir /var/log/efax ; chmod 755 /var/log/efax )
