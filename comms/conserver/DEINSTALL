#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1.1.1.2.2 2002/06/23 18:41:25 jlam Exp $

PKGNAME=$1
STAGE=$2

CAT="@CAT@"

case ${STAGE} in
DEINSTALL)
	;;

POST-DEINSTALL)
	${CAT} << EOF
===========================================================================
If you won't be using ${PKGNAME} any longer, you may want
to remove the following directories:

	/var/console
===========================================================================
EOF
	;;

*)
	echo "Unexpected argument: ${STAGE}"
	exit 1
	;;
esac
exit 0
