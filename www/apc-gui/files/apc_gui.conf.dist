# APC-GUI configuration file fragment for Apache

<IfModule mod_alias.c>
    Alias /apc_gui/ "@PREFIX@/@APC_GUI_DIR@/"
</IfModule>

<Directory "@PREFIX@/@APC_GUI_DIR@">
    Options Indexes MultiViews
    AllowOverride None
    Order allow,deny
    Allow from all
</Directory>
