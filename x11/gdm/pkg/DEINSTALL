#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1.1.1 1999/11/10 23:15:28 tron Exp $

if [ "$2" != POST-DEINSTALL ]
then
	cd @@GDMCFGDIR@@
	@@RM@@ -f Sessions/Default
	for SOURCE in @@GDMCFGFILES@@
	do
		TARGET=${SOURCE%.default*}${SOURCE##*.default}
		if cmp -s $SOURCE $TARGET
		then
			@@RM@@ -f $TARGET
		fi
	done
fi

exit 0
