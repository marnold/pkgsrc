+#ifdef __NetBSD__
+int
+getresuid(uid_t *ruid, uid_t *euid, uid_t *suid)
+{
+	*ruid = getuid();
+	*euid = geteuid();
+	*suid = *euid;
+}
+int
+getresgid(gid_t *rgid, gid_t *egid, gid_t *sgid)
+{
+	*rgid = getgid();
+	*egid = getegid();
+	*sgid = *egid;
+}
+#else
 int getresuid(uid_t *ruid, uid_t *euid, uid_t *suid);
 int getresgid(gid_t *rgid, gid_t *egid, gid_t *sgid);
+#endif
 
