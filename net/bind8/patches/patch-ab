*** 228,241 ****
  #
  # User command manual entries
  #
! CMD_BASE = 	dig host dnsquery dnskeygen
  CMD_SRC_EXT = 	1
  CMD_SRC = 	dig.${CMD_SRC_EXT} \
- 		host.${CMD_SRC_EXT} \
  		dnsquery.${CMD_SRC_EXT} \
  		dnskeygen.${CMD_SRC_EXT}
  CMD_OUT =	dig.${CMD_OUT_EXT} \
- 		host.${CMD_OUT_EXT} \
  		dnsquery.${CMD_OUT_EXT} \
  		dnskeygen.${CMD_OUT_EXT}
  
